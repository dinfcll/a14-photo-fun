@{
    ViewBag.Title = "Animaux";
    List<string> lstimage = ViewData["lstimage"] as List<string>;
}
<div class="page-header">
    <h2>Animaux</h2>
</div>
    @using(Html.BeginForm()){
    if (lstimage != null && lstimage.Count > 0)
    {
        foreach (var PhotoAnimal in lstimage)
        {
            <img src="@PhotoAnimal" alt="Image inaccessible" width="36%"/>
        }
    }
}

