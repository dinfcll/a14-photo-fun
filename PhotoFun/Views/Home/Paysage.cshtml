@{
    ViewBag.Title = "Paysage";
    var lstimage = ViewData["lstimage"] as List<string>;
}

<div class="page-header">
    <h2>Paysage</h2>
</div>
@using(Html.BeginForm()){
    if (lstimage != null && lstimage.Count > 0)
    {
        foreach (var PhotoPaysage in lstimage)
        {
            <img src="@PhotoPaysage" class="img-thumbnail" alt="Image inaccessible" width="36%"/>
        }
    }
    else
    {
        @Html.Partial("_PasPhotoCategorie")
    }
}
