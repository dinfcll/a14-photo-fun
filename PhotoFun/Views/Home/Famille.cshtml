@{
    var lstimage = ViewData["lstimage"] as List<string>;
}
<div class="page-header">
    <h2>Famille</h2>
</div>
@using(Html.BeginForm()){
    if (lstimage != null && lstimage.Count > 0)
    {
        foreach (var PhotoFamille in lstimage)
        {
            <img src="@PhotoFamille" class="img-thumbnail" alt="Image inaccessible" width="36%"/>
        }
    }
    else
    {
         @Html.Partial("_PasPhotoCategorie")
    }
}
