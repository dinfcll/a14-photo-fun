<hgroup class="title">
    <h1>Importer des photos</h1>
</hgroup>
<article>
@using (Html.BeginForm("Upload", "Importer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{ 
    <input type="file" name="file" />
    
}
   <div>
        <p>Catégorie</p>
        <ol>
             <input type="radio" name="categorie" />Sport
             <input type="radio" name="categorie" />Nature
             <input type="radio" name="categorie" />Famille
             <input type="radio" name="categorie" />Paysage
             <input type="radio" name="categorie" />Cuisine
             <input type="radio" name="categorie" />Animaux
             <input type="radio" name="categorie" />Autres
        </ol>
   </div>
<input type="submit" name="SubImporter" value="Importer" />

</article>

<aside>
        <ul>
        <li>@Html.ActionLink("Accueil", "Index", "Home")</li>
        <li>@Html.ActionLink("Importer", "Importer", "Home")</li>
        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
    </ul>
</aside>
