@{
    ViewBag.Title = "Vos Photos";
    var lstimage = ViewData["lstimage"] as List<string>;
}
@{ 
    <!--??-->
    @model PhotoFun.Models.PhotoModels
    <div class="page-header">
        <h2>Vos photos</h2>
    </div>
    if (lstimage != null && lstimage.Count > 0)
    {
        foreach (var PhotoUtil in lstimage)
        {
            using (Html.BeginForm("PhotoUtil", "Account", FormMethod.Post))
            {
                <div style="width:35%">
                    <img src="@PhotoUtil" class="img-thumbnail" alt="Image inaccessible" width="89%"/>
                    @Html.HiddenFor(m => m.image)
                    <button type="submit" class="close" id="@PhotoUtil"><span class="glyphicon glyphicon-remove"></span></button>
                </div>
            }
        }
    }
}
